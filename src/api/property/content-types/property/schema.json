{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "imageGallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "floorPlanImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "masterPlanImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "startPrice": {
      "type": "decimal"
    },
    "propertyType": {
      "type": "enumeration",
      "enum": [
        "Apartment",
        "Villa",
        "Twinhouse",
        "Townhouse",
        "Duplex",
        "Penthouse",
        "Chalet",
        "Studio",
        "Cabin",
        "Clinic",
        "Office"
      ]
    },
    "bathrooms": {
      "type": "integer"
    },
    "bedrooms": {
      "type": "integer"
    },
    "squareMeters": {
      "type": "integer"
    },
    "deliveryIn": {
      "type": "integer"
    },
    "finishing": {
      "type": "enumeration",
      "enum": [
        "Not Finished",
        "Semi Finished",
        "Finished",
        "Furnished"
      ]
    },
    "amenities": {
      "type": "customField",
      "default": "[]",
      "customField": "plugin::multi-select.multi-select",
      "options": [
        "Garden",
        "Has roof",
        "bicycles lanes",
        "Disability support",
        "Jogging trail",
        "Outdoor pools",
        "Mosque",
        "Sports Clubs",
        "Business Hub",
        "Commercial strip",
        "Medical center",
        "Schools",
        "Underground parking",
        "Clubhouse"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "isSoldout": {
      "type": "boolean",
      "default": false
    },
    "isRecommended": {
      "type": "boolean",
      "default": false
    },
    "locationOnMap": {
      "type": "customField",
      "options": {
        "info": true
      },
      "customField": "plugin::geodata.geojson"
    },
    "compound": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::compound.compound",
      "inversedBy": "properties"
    },
    "maxPrice": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    }
  }
}
