{
  "kind": "collectionType",
  "collectionName": "areas",
  "info": {
    "singularName": "area",
    "pluralName": "areas",
    "displayName": "Destinations",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "idField": "slug"
  },
  "pluginOptions": {},
  "attributes": {
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "compounds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::compound.compound",
      "mappedBy": "area"
    },
    "description": {
      "type": "text"
    },
    "developers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::developer.developer",
      "inversedBy": "areas"
    },
    "isRecommended": {
      "type": "boolean",
      "default": false
    },
    "about": {
      "type": "text"
    }
  }
}
